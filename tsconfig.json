{

    "compilerOptions": {
      "target": "es5",
      "module": "es2015",
      "moduleResolution": "node",
      "sourceMap": true,
      "inlineSources": true,
      "declaration": true,
      "experimentalDecorators": true,
      "noImplicitAny": false,
      "suppressImplicitAnyIndexErrors": true,
      "skipLibCheck": true,
      "stripInternal": true,
      "lib": [
          "es2015",
          "dom"
      ],
      "baseUrl": ".",
      "rootDir": "./src",
      "outDir": "./build",
      "types": []
    },
    "files": [
      "./src/my.service.ts"
    ],
    "angularCompilerOptions": {
      "flatModuleOutFile": "demo.js",
      "flatModuleId": "@ngc/demo",
      "annotateForClosureCompiler": true,
      "strictMetadataEmit": true,
      "skipTemplateCodegen": true,
      "allowEmptyCodegenFiles": false
    }
  }